{
  "name": "god-digital-marketing-premium",
  "version": "1.0.0",
  "description": "Revolutionary 4D Digital Marketing Website with Premium Black & White Design",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=/ --watch=css,js,index.html",
    "build": "npm run clean && npm run optimize:css && npm run optimize:js && npm run optimize:images && npm run generate:sw",
    "build:production": "npm run build && npm run test:full && npm run deploy:prepare",
    "clean": "rimraf dist && mkdir -p dist/{css,js,assets,reports}",
    "optimize:css": "postcss css/*.css --use autoprefixer cssnano --dir dist/css/ && npm run inline:critical",
    "optimize:js": "terser js/*.js --compress --mangle --output dist/js/bundle.min.js && npm run generate:sourcemaps",
    "optimize:images": "imagemin assets/images/**/*.{jpg,png} --out-dir=dist/assets/images && npm run generate:webp",
    "generate:webp": "cwebp -q 85 assets/images/**/*.{jpg,png} -o dist/assets/images/",
    "inline:critical": "critical index.html --base ./ --inline --minify --extract --width 1300 --height 900",
    "generate:sourcemaps": "terser js/*.js --source-map --output dist/js/bundle.min.js.map",
    "generate:sw": "workbox generateSW workbox-config.js",
    "test": "npm run test:lighthouse && npm run test:accessibility && npm run test:performance",
    "test:full": "npm run test:foundation && npm run test:design && npm run test:responsive && npm run test:performance && npm run test:seo",
    "test:foundation": "node tests/run-foundation-tests.js",
    "test:design": "node tests/run-design-tests.js",
    "test:responsive": "node tests/run-responsive-tests.js",
    "test:performance": "lighthouse http://localhost:3000 --output=json --output-path=./reports/lighthouse.json && npm run analyze:bundle",
    "test:accessibility": "axe http://localhost:3000 --save ./reports/accessibility.json",
    "test:seo": "node tests/run-seo-tests.js",
    "analyze:bundle": "webpack-bundle-analyzer dist/js/bundle.min.js --mode static --report dist/reports/bundle-analysis.html",
    "deploy:prepare": "npm run compress:assets && npm run generate:manifest && npm run validate:production",
    "compress:assets": "gzip -9 -c dist/css/*.css > dist/css/styles.css.gz && gzip -9 -c dist/js/*.js > dist/js/bundle.js.gz",
    "generate:manifest": "node scripts/generate-manifest.js",
    "validate:production": "node scripts/validate-production.js",
    "serve:production": "http-server dist -p 8080 -g --cors",
    "monitor:performance": "node scripts/performance-monitor.js",
    "report:generate": "node scripts/generate-report.js"
  },
  "keywords": [
    "digital-marketing",
    "4d-website",
    "premium-design",
    "webgl",
    "three.js",
    "performance-optimized"
  ],
  "author": "God Digital Marketing",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "axe-cli": "^4.8.2",
    "critical": "^6.1.0",
    "cssnano": "^6.0.1",
    "cwebp-bin": "^8.0.0",
    "http-server": "^14.1.1",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "lighthouse": "^11.4.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "rimraf": "^5.0.5",
    "terser": "^5.26.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "workbox-cli": "^7.0.0"
  },
  "dependencies": {
    "three": "^0.159.0",
    "gsap": "^3.12.2",
    "lenis": "^1.0.42",
    "intersection-observer": "^0.12.2",
    "@types/three": "^0.159.0",
    "dat.gui": "^0.7.9",
    "stats.js": "^0.17.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
